{
  "name": "meet-normal-bedbug",
  "displayName": "Meet normal bedbug",
  "version": "0.0.1",
  "description": "A demo extension for xTen.",
  "author": "ruslanaljabari",
  "scripts": {
    "dev": "plasmo dev --verbose",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "@plasmohq/messaging": "^0.3.3",
    "file-saver": "^2.0.5",
    "openai": "^3.2.1",
    "plasmo": "0.68.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "xten-pkg": "link:../@xten"
  },
  "devDependencies": {
    "@plasmohq/prettier-plugin-sort-imports": "3.6.4",
    "@types/chrome": "0.0.227",
    "@types/node": "18.15.11",
    "@types/react": "18.0.32",
    "@types/react-dom": "18.0.11",
    "prettier": "2.8.7",
    "typescript": "5.0.3"
  },
  "manifest": {
    "host_permissions": [
      "https://*/*"
    ],
    "background": {
      "scripts": [
        "contents/summary.ts"
      ],
      "persistent": true
    },
    "permissions": [
      "background",
      "scripting",
      "history",
      "storage",
      "contextMenus",
      "activeTab",
      "tabs"
    ]
  }
}
