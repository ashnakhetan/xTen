{
  "manifest_version": 3,
  "name": "Base-React - esbuild",
  "version": "0.1.0",
  "author": "F what you heard it's me ya boy",
  "description": "base react extention - esbuild",
  "permissions": [
    "tabs",
    "webNavigation",
    "activeTab"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "base react extention - esbuild"
  },
  "host_permissions": [
    "https://*/*"
  ],
  "background": {
    "service_workers": ["build/background.js"]
  },
  "icons": {
    "192": "logo192.png",
    "512": "logo512.png"
  },
  "web_accessible_resources": [{
    "resources": ["build/injected.js"],
    "matches": ["<all_urls>"]
  }],
  "content_scripts": [
    {
      "matches": ["file://*/*", "http://*/*", "https://*/*"],
      "js": ["build/content.js"],
      "run_at": "document_end"
    }
  ]
}
